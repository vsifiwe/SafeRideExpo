        "googleMaps": {
          "apiKey": "AIzaSyDDlCnqPq35TgKh6Jg8yt3no3cXSsZowfs"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
